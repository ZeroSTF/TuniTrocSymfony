{% extends 'template.html.twig' %}


{% block body %}
    <h1>Fidelite index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Valeur</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for fidelite in fidelites %}
            <tr>
                <td>{{ fidelite.id }}</td>
                <td>{{ fidelite.valeur }}</td>
                <td>
                    <a href="{{ path('app_fidelite_show', {'id': fidelite.id}) }}">show</a>
                    <a href="{{ path('app_fidelite_edit', {'id': fidelite.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_fidelite_new') }}">Create new</a>
{% endblock %}
